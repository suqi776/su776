{
  "version": 2,
  "build": {
    "src": "package.json",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "dist"
    },
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "headers": [
    {
      "source": "/assets/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "permanent": false
    }
  ]
}
